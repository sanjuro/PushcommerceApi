{
    "name": "pushcommerce/php-pushcommerce-api",
    "type": "library",
    "description": "Api Wrapper for PushCommerce API",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "proprietary",
    "authors": [
        {
            "name": "Shadley Wentzel",
            "email": "shadley@pushcommerce.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.3",
        "php-http/cache-plugin": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "php-http/guzzle6-adapter": "^1.0",
        "guzzlehttp/psr7": "^1.2",
        "sllh/php-cs-fixer-styleci-bridge": "^1.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sanjuro/PushcommerceApi.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pushcommerce\\": "src"
        }
    }
}